-- Scripts:

local function UFCLYWU_fake_script() -- DoubleWeights.Weight 
	local script = Instance.new('LocalScript', DoubleWeights)

	
	repeat
		wait(.1)
		local num = 0
		for i,v in pairs(game:GetService("CoreGui").RobloxGui.Backpack.Inventory.ScrollingFrame.UIGridFrame:GetChildren()) do
			if v:IsA("TextButton") then
				num += 1
			end
		end
		DoubleWeights.Text = num.."Weights"
	until num == 9999999999999
end
coroutine.wrap(UFCLYWU_fake_script)()
local function KDSXOJ_fake_script() -- X.LocalScript 
	local script = Instance.new('LocalScript', X)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
	
	
	
	
end
coroutine.wrap(KDSXOJ_fake_script)()
local function BEACF_fake_script() -- lagX.LocalScript 
	local script = Instance.new('LocalScript', lagX)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
	
	
	
	
end
coroutine.wrap(BEACF_fake_script)()
local function OIBSIB_fake_script() -- Gained.LocalScript 
	local script = Instance.new('LocalScript', Gained)

	local str = game:GetService("Players").LocalPlayer.leaderstats.Strength;
	local curvalue = str.Value;
	local earned = 0;
	str:GetPropertyChangedSignal("Value"):Connect(function()
		local delta = (str.Value - curvalue);
		earned += delta;
		Gained.Text = "Strength Earned: "..earned;
		curvalue = str.Value;
	end)
end
coroutine.wrap(OIBSIB_fake_script)()

Doubleweightclicklift.MouseButton1Click:Connect(function()
	_G.Auto4 = true
	while _G.Auto4 do
		wait(4.63)
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == "Double Weight" then v:Activate() end

		end
	end

	Doubleweightclicklift.Visible = true
	Doubleweightclicklift.Visible = false
	_G.farm = false
end)

DoubleWeightslowfarm.MouseButton1Click:Connect(function()
	DoubleWeightslowfarm.Visible = true
	DoubleWeightslowfarm.Visible = false
	_G.farm = true
	while _G.farm do
		wait(0.7)
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == TextBox.Text then v:Activate() end
		end
	end

	DoubleWeightslowfarm.Visible = true
	DoubleWeightslowfarm.Visible = false
	_G.farm = false
end)

