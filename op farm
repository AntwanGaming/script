Equiptool.MouseButton1Click:Connect(function()
	_G.equip = true
	_G.unequip = false
	Equiptool.Visible = false
	unEquiptool.Visible = true
	while _G.equip do
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v.Name == TextBox.Text then
				v.Parent = game.Players.LocalPlayer.Character
			end
		end
	end
end)

